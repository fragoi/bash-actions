#!/bin/bash -e

## Allows a script to use the same name of a function.

. setEnv
. "$V_FUNCS"

__functionWrapper_func=$1

[[ "$__functionWrapper_func" ]] || {
  echo 1>&2 "Usage: $0 <function> [parameters]"
  exit 2
}

if [[ $(type -t "$__functionWrapper_func") != "function" ]]; then
  echo 1>&2 "function \"${__functionWrapper_func}\" not defined"
  exit 1
fi

shift

$__functionWrapper_func "$@"
